	option name 'Allow-SmartDNS-DoT-8753'
	option src 'lan'
	option proto 'tcp'
	option dest_port '8753'
	option target 'ACCEPT'
	option family 'ipv4'
	option dest_ip '192.168.3.2'
	option dest 'lan'

